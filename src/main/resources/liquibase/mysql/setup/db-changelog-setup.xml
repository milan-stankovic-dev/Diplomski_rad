<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <property name="defaultSchemaName" value="public"/>
        <changeSet id="create_order_queue" author="milan s.">
            <sqlFile path="ddl/create_order_queue.sql"
                     relativeToChangelogFile="true" />
        </changeSet>
        <changeSet id="create_order_xml_procedure" author="milan s.">
            <sqlFile path="ddl/create_order_xml_procedure.sql"
                     relativeToChangelogFile="true" splitStatements="false"/>
        </changeSet>
         <changeSet id="save_invoice" author="milan s.">
            <sqlFile path="dml/save_invoice_function.sql"
                     relativeToChangelogFile="true" splitStatements="false"/>
        </changeSet>
        <changeSet id="create_invoice_check" author="milan s.">
            <sqlFile path="ddl/create_procedure_check_for_invoice.sql"
                     relativeToChangelogFile="true" splitStatements="false"/>
        </changeSet>
        <changeSet id="create_invoice_trigger" author="milan s.">
            <sqlFile path="ddl/create_invoice_trigger.sql" relativeToChangelogFile="true"/>
        </changeSet>
</databaseChangeLog>